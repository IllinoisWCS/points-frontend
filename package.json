{
  "name": "points-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://points.illinoiswcs.org",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier-format": "prettier --config .prettierrc --write",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b main -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/theme-utils": "^2.0.21",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-table": "^8.8.4",
    "@types/react-router": "^5.1.20",
    "axios": "^1.2.3",
    "cors": "^2.8.5",
    "framer-motion": "^8.5.0",
    "gh-pages": "^6.1.1",
    "moment": "^2.29.4",
    "qrcode.react": "^4.1.0",
    "react": "^18.2.0",
    "react-confetti": "^6.2.2",
    "react-chakra-pagination": "^2.0.3",
    "react-dark-mode-toggle-2": "^2.0.8",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.7.1",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.7.0",
    "react-use": "^17.6.0",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-table": "^7.7.14",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
